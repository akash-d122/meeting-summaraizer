{
  "name": "meeting-summarizer",
  "version": "1.0.0",
  "description": "AI-powered meeting summarizer with Groq integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "task-master": "task-master",
    "db:init": "node scripts/init-database.js",
    "db:init:sample": "node scripts/init-database.js --with-sample-data",
    "db:reset": "node scripts/init-database.js --reset",
    "db:test": "node -e \"require('./config/database').testConnection()\"",
    "db:test:full": "node scripts/test-database.js",
    "test:upload": "node scripts/test-upload-simple.js",
    "test:instructions": "node scripts/test-instructions-api.js",
    "test:groq": "node scripts/test-groq-credentials.js"
  },
  "keywords": [
    "meeting",
    "summarizer",
    "ai",
    "groq",
    "taskmaster"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.4",
    "groq-sdk": "^0.3.3",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "sequelize": "^6.35.0",
    "sqlite3": "^5.1.7",
    "task-master-ai": "^0.24.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
